{
  "name": "audio-master-electron",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "dev": "electron-vite --mode test",
    "dev:test": "electron-vite --mode test",
    "dev:production": "electron-vite --mode production",
    "build:test": "pnpm typecheck && electron-vite build --mode test",
    "build:production": "pnpm typecheck && electron-vite build --mode production",
    "build:report": "cross-env REPORT=true electron-vite build --mode production",
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm build:production && electron-builder --dir",
    "build:win": "pnpm build:production && electron-builder --win",
    "build:mac": "pnpm build:production && electron-builder --mac",
    "build:linux": "pnpm build:production && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@vime/core": "^5.4.1",
    "@vime/vue-next": "^5.4.1",
    "axios": "^1.6.7",
    "js-base64": "^3.7.7",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.8.1",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "cross-env": "^7.0.3",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.7.1",
    "@types/node": "^18.19.9",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}
